<p id="notice"><%= notice %></p>

<h1>Customizes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Slogan</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customizes.each do |customize| %>
      <tr>
        <td><%= customize.title %></td>
        <td><%= customize.slogan %></td>
        <td><%= customize.description %></td>
        <td><%= link_to 'Show', customize %></td>
        <td><%= link_to 'Edit', edit_customize_path(customize) %></td>
        <td><%= link_to 'Destroy', customize, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customize', new_customize_path %>
